{
  "name": "QRCoder",
  "version": "0.1.0",
  "summary": "A QR code generator and reader for OSX and iOS written in Swift.",
  "description": "Since OSX 10.9 apple offers a CI filter to generate QR codes.\nHowever, scaling the QR code to the desired size without blurring\nthe image doesn't work out of the box. That's why I created this library.\n",
  "homepage": "https://github.com/sbhklr/QRCodeGenerator",
  "license": "MIT",
  "authors": {
    "Sebastian Hunkeler": "sebastian.hunkeler@iml.unibe.ch"
  },
  "source": {
    "git": "https://github.com/sbhklr/QRCodeGenerator.git",
    "tag": "0.1.0"
  },
  "social_media_url": "https://twitter.com/sbhklr",
  "requires_arc": true,
  "ios": {
    "source_files": "Pod/Classes/*.swift",
    "frameworks": [
      "Foundation",
      "UIKit",
      "QuartzCore"
    ]
  },
  "osx": {
    "source_files": "Pod/Classes/QRCodeGenerator.swift",
    "frameworks": [
      "Foundation",
      "AppKit",
      "QuartzCore"
    ]
  },
  "platforms": {
    "ios": "8.0",
    "osx": "10.9"
  }
}
